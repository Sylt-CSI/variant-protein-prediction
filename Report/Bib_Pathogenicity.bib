	file = {Baugh et al. - 2016 - SUPPLEMENTARY Robust classification of protein va.pdf:/Users/gcc/Zotero/storage/PS2W5A29/Baugh et al. - 2016 - SUPPLEMENTARY Robust classification of protein va.pdf:application/pdf}
